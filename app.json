{
  "expo": {
    "name": "Pinner",
    "slug": "pinner-plusbeauxjours",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "scheme": "pinner.plusbeauxjours",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "pinner.plusbeauxjours",
      "infoPlist": {
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Pinner uses your location for features like Filters, Search, Pinner Map, and more.",
        "NSLocationWhenInUseUsageDescription": "Pinner uses your location for features like Filters, Search, Pinner Map, and more.",
        "NSPhotoLibraryUsageDescription": ""
      }
    },
    "android": {
      "package": "pinner.plusbeauxjours",
      "permissions": [
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "notification": {
      "icon": "./assets/notification.png"
    },
    "description": ""
  }
}